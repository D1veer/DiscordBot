{
    "name": "jaxkdev/discord-bot",
    "type": "project",
    "description": "A PocketMine-MP Plugin that hosts your very own discord bot using DiscordPHP.",
    "license": "OSL-3.0",
    "authors": [
        {
            "name": "JaxkDev",
            "email": "JaxkDev@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "JaxkDev\\DiscordBot\\": "src/JaxkDev/DiscordBot"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/JaxkDev/DiscordPHPSlim"
        }
    ],
    "require": {
        "php": "^7.3",
        "team-reflex/discord-php": "dev-beta"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.64",
        "pocketmine/pocketmine-mp": "^3"
    },
    "scripts": {
        "phpstan": "phpstan analyse"
    }
}
