includes:
    - vendor/phpstan/phpstan-strict-rules/rules.neon

parameters:
    level: max
    paths:
        - src
    excludePaths:
        analyse:
            - vendor
            - src/Libs
            - src/Bot
    bootstrapFiles:
        - test/phpstan/bootstrap.php
    checkMissingIterableValueType: false
    treatPhpDocTypesAsCertain: false