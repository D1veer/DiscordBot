parameters:
    level: max
    paths:
        - src
    excludePaths:
        analyse:
            - vendor
            - src/Communication/Packets/*/*Message*.php
            - src/Communication/Packets/*/*Channel*.php
            - src/Libs
    bootstrapFiles:
        - test/phpstan/bootstrap.php
    checkMissingIterableValueType: false
    treatPhpDocTypesAsCertain: false